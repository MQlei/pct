#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vue-demi@0.13.11_vue@3.5.18_typescript@5.9.2_/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../../../../../vue-demi@0.13.11_vue@3.5.18_typescript@5.9.2_/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
fi
